        {% include base_path ~ directory ~ "/bem/blocks/subscription/subscription.html.twig" %}
        {% include base_path ~ directory ~ "/bem/blocks/actions/actions.html.twig" %}
        {% include base_path ~ directory ~ "/bem/blocks/reviews/reviews.html.twig" %}
