            <div class="service__content">
                {{ content.main }}

<div class="faq-blk content_norm ">
{{ drupal_view("faqservice","block_1") }}
</div>


       



            </div>

            


        </div>

        {% include base_path ~ directory ~ "/bem/blocks/stomatology/stomatologyService.html.twig" %}
